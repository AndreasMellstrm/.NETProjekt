@model IEnumerable<NETDatingApp.Models.UserPostModel>

@{
    Layout = null;
}

<table class="table">
    <tr>
        <th>Id</th>
        <th>User</th>
        <th>Comment</th>
        <th>Img</th>
    </tr>

    @foreach (var m in Model) {
    <tr>
        <td>@m.Id</td>
        <td>@m.User</td>
        <td>@m.Comment</td>
    </tr>

    <button type="button" class="btn btn-success" id="submit"> Submit </button>
    }
</table>